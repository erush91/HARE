<launch>
        <node pkg="depth"
	type="depth"
	name="depth"
        output="screen"
        ns="robot1">
                <param  name="FLAG_DEPTH"
                        value="1" />
                <param  name="FLAG_COLOR"
                        value="1" />
                <param  name="FLAG_INFRARED"
                        value="1"/>

                        
                <!-- fixed map frame (set to 'map' if SLAM or localization running!) -->
                <param  name="frame_id"
                        type="string"
                        value="world" />
                <param  name="sensor_model/max_range" 
                        value="5.0" />
        </node>
</launch>